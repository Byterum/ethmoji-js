{
  "name": "ethmoji-js",
  "version": "0.0.1",
  "description": "Add an Ethmoji avatar for users of your dapp",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rm -rf lib",
    "build:lib": "babel src --out-dir lib",
    "prepublish": "NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProjectOpenSea/ethmoji-avatar.git"
  },
  "keywords": [
    "ethereum",
    "ethmoji",
    "smart-contracts"
  ],
  "author": "support@opensea.io",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ProjectOpenSea/ethmoji-js/issues"
  },
  "homepage": "https://github.com/ProjectOpenSea/ethmoji-js#readme",
  "dependencies": {
    "ethmoji-contract": "https://github.com/dfinzer/ethmoji-contracts",
    "truffle-contract": "https://github.com/ProjectOpenSea/truffle-contract",
    "web3": "^0.20.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  }
}
